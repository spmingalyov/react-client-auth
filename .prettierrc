{
	"trailingComma": "none",
	"tabWidth": 2,
	"useTabs": true,
	"semi": false,
	"singleQuote": true,
	"jsxSingleQuote": true,
	"arrowParens": "avoid",
	"importOrder": [
		"^react/(.*)$",
		"<THIRD_PARTY_MODULES>",
		"^@pages/(.*)$",
		"^@components/(.*)$",
		"^@store/(.*)$",
		"^@helpers/(.*)$",
		"^@hooks/(.*)$",
		"^@utils/(.*)$",
		"^[./]",
		"^[../]"
	],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true,
	"plugins": ["@trivago/prettier-plugin-sort-imports"]
}
